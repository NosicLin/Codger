class rectange
	private attr width
	private attr height
	protected attr name="rectangle"

	func init(w,h)
		this.width=w
		this.height=h
	end 

	func area()
		return this.width*this.height
	end 

	func setWidth(w)
		this.width=w
	end 

	func setHeight(h)
		this.height=h
	end 

	func getName()
		return this.name
	end 
		
end 


print "begin"
c1=rectange(3,4)
print c1.area()

c1.setWidth(7)
c1.setHeight(8)
print c1.area()


c1.name="r22"
print c1.getName()
